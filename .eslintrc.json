{
	"extends": ["prettier", "plugin:@next/next/recommended"],
	"rules": {
		"react/no-unknown-property": ["error", { "ignore": ["jsx"] }]
	},
	"env": {
		"browser": true,
		"es2021": true
	},
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["prettier"]
}
